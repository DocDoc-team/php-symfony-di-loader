{
    "name": "docdoc/symfony-di-loader",
    "description": "Simple loader for symfony DI container",
    "homepage": "https://github.com/docdoc/php-symfony-di-loader",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "DI",
        "Symfony",
        "Factory",
        "Container",
        "PSR11"
    ],
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|~8.0",
        "ext-json": "*",
        "symfony/dependency-injection": "~5.2",
        "symfony/yaml": "^5.2",
        "symfony/config": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "DocDoc\\SymfonyDiLoader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DocDoc\\SymfonyDiLoader\\Unit\\": "test/unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mikey179/vfsstream": "^1.6"
    }
}
